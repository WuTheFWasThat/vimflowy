{
  "name": "vimflowy",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "0.10.x",
    "npm": "1.2.x"
  },
  "scripts": {
    "build": "NODE_ENV=production webpack --config config/webpack.prod.js",
    "coverage": "multi='spec=- html-cov=public/coverage.html' node_modules/.bin/mocha  --compilers js:babel-core/register test/tests --require blanket --reporter mocha-multi test/tests/*",
    "start": "node dev_server.js",
    "test": "node_modules/.bin/mocha --require ts-babel-node/register --require babel-polyfill --compilers js:babel-core/register,ts:ts-node/register,tsx:ts-node/register test/tests",
    "typecheck": "tsc -p . --noEmit"
  },
  "config": {
    "blanket": {
      "pattern": "//assets/js/.*\\.js$/"
    }
  },
  "dependencies": {
    "font-awesome": "^4.6.3",
    "jquery": "^3.1.0",
    "lodash": "^4.15.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1"
  },
  "devDependencies": {
    "babel-core": "6.4.5",
    "babel-eslint": "^6.1.2",
    "babel-loader": "6.2.1",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-3": "^6.11.0",
    "babel-runtime": "^6.11.6",
    "blanket": "^1.2.3",
    "css-loader": "^0.23.1",
    "express": "^4.14.0",
    "file-loader": "^0.9.0",
    "mocha": "^3.0.2",
    "mocha-multi": "^0.9.1",
    "node-sass": "^3.8.0",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "ts-babel-node": "^1.0.0",
    "ts-loader": "^0.8.1",
    "ts-node": "^1.3.0",
    "tslint": "^3.3.0",
    "tslint-loader": "^2.1.0",
    "typescript": "^1.7.5",
    "webpack": "^1.12.15",
    "webpack-async-await": "^1.0.4",
    "webpack-dev-server": "^1.12.1"
  }
}
