{
  "name": "vimflowy",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "0.10.x",
    "npm": "1.2.x"
  },
  "scripts": {
    "start": "node_modules/.bin/gulp",
    "test": "node_modules/.bin/mocha test/tests/*",
    "coverage": "multi='spec=- html-cov=public/coverage.html' node_modules/.bin/mocha --require blanket --reporter mocha-multi test/tests/*",
    "gulp": "gulp"
  },
  "config": {
    "blanket": {
      "pattern": "//assets/js/.*\\.coffee$/",
      "loader": "./node-loaders/coffee-script"
    }
  },
  "dependencies": {
    "async": "latest",
    "coffee-script": "latest",
    "express": "latest",
    "lodash": "latest"
  },
  "devDependencies": {
    "blanket": "latest",
    "mocha": "latest",
    "mocha-multi": "latest",

    "del": "latest",
    "gulp": "latest",
    "gulp-coffee": "latest",
    "gulp-jade": "latest",
    "gulp-mocha": "latest",
    "gulp-rename": "latest",
    "gulp-sass": "latest",
    "gulp-sourcemaps": "latest",
    "gulp-util": "latest",
    "stream-to-array": "latest"
  }
}
