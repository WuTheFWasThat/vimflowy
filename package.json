{
  "name": "vimflowy",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "0.10.x",
    "npm": "1.2.x"
  },
  "scripts": {
    "build": "NODE_ENV=production webpack --config config/webpack.prod.js",
    "coverage": "multi='spec=- html-cov=public/coverage.html' node_modules/.bin/mocha  --compilers js:babel-core/register test/tests --require blanket --reporter mocha-multi test/tests/*",
    "start": "node dev_server.js",
    "test": "node_modules/.bin/mocha --compilers js:babel-core/register test/tests"
  },
  "config": {
    "blanket": {
      "pattern": "//assets/js/.*\\.js$/"
    }
  },
  "dependencies": {
    "async": "latest",
    "express": "latest",
    "lodash": "latest",
    "react": "latest",
    "react-dom": "latest",
    "ts-loader": "^0.8.1",
    "tv4": "latest",
    "typescript": "^1.7.5"
  },
  "devDependencies": {
    "babel-core": "6.4.5",
    "babel-eslint": "latest",
    "babel-loader": "6.2.1",
    "babel-plugin-transform-runtime": "latest",
    "babel-preset-es2015": "latest",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-3": "latest",
    "babel-runtime": "latest",
    "babelify": "latest",
    "blanket": "latest",
    "browserify": "latest",
    "css-loader": "^0.23.1",
    "mocha": "latest",
    "mocha-multi": "latest",
    "node-sass": "latest",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "tslint": "^3.3.0",
    "tslint-loader": "^2.1.0",
    "webpack": "^1.12.15",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.6.4"
  }
}
